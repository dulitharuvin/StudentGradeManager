<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ICourseModuleService" />
                <binding name="BasicHttpBinding_ICourseService" />
                <binding name="BasicHttpBinding_IResultService" />
                <binding name="BasicHttpBinding_IModuleAssessmentService" />
                <binding name="BasicHttpBinding_IStudentService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/StudentGradeManagerService/CourseModuleService/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICourseModuleService"
                contract="CourseModuleServiceReference.ICourseModuleService"
                name="BasicHttpBinding_ICourseModuleService" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/StudentGradeManagerService/CourseService/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICourseService"
                contract="CourseServiceReference.ICourseService" name="BasicHttpBinding_ICourseService" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/StudentGradeManagerService/ResultService/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IResultService"
                contract="ResultServiceReference.IResultService" name="BasicHttpBinding_IResultService" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/StudentGradeManagerService/ModuleAssessmentService/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IModuleAssessmentService"
                contract="ModuleAssessmentServiceReference.IModuleAssessmentService"
                name="BasicHttpBinding_IModuleAssessmentService" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/StudentGradeManagerService/Service1/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IStudentService"
                contract="StudentServiceReference.IStudentService" name="BasicHttpBinding_IStudentService" />
        </client>
    </system.serviceModel>
</configuration>